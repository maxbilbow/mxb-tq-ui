{
    "deno.enable": true,
    "deno.lint": true,
    "deno.unstable": false,
    "deno.importMap": "./importmap.json",
    "deno.config": "./deno.json",
    "files.associations": {
        "*.tpl": "html"
    },
    "explorer.experimental.fileNesting.patterns": {
        "*.ts": "${capture}.js, ${capture}.tpl, ${capture}.*.tpl, tsconfig.${capture}.json",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.tsx": "${capture}.ts",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, yarn.lock, .gitignore",
        "index.html": "*.ico, *.ts, *.webmanifest, tsconfig.json"
    }
}